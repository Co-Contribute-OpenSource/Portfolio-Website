{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off", 
        "link-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "overscroll-behavior-inline",
          "overscroll-behavior",
          "backdrop-filter"
        ]
      }
    ],
    "css-prefix-order": "off",
    "disown-opener": "off"
  }
}